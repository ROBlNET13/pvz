{
	"$schema": "./node_modules/oxfmt/configuration_schema.json",
	"tabWidth": 4,
	"useTabs": true,
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"printWidth": 160,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"semi": true,
	"singleAttributePerLine": false,
	"singleQuote": false,
	"trailingComma": "es5",
	"ignorePatterns": ["game/js/pako.js", "game/js/html2canvas.js", "game/js/msgpack.min.js", ".github/workflows", "pnpm-lock.yaml"]
}
